ls=["gmail.com","yahoo.com","twitter.com","facebook.com"]
em=input("Enter An Email: ")

def invalid():
    print("Invalid Email")

s=em.split('@')
l=len(s)
if l!=2:
    invalid()
else:
    g=s[0].lower()
    if g in ls:
        invalid()
    else:
        a=s[1].lower()
        if a in ls:
            print("Valid Email")
        else:
            invalid()
